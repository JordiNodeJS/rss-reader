{
	"servers": {
		"context7": {
			"type": "http",
			"url": "https://mcp.context7.com/mcp",
			"headers": {
				"CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
			}
		},
		"chrome-devtools": {
			"command": "npx",
			"args": [
				"-y",
				"chrome-devtools-mcp"
			],
			"type": "stdio"
		},
		"next-devtools": {
			"command": "npx",
			"args": [
				"-y",
				"next-devtools-mcp@latest"
			],
			"type": "stdio"
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		},
		"shadcn": {
			"command": "npx",
			"args": [
				"shadcn@latest",
				"mcp"
			],
			"type": "stdio"
		},
		"postgres": {
			"command": "npx",
			"args": [
				"@modelcontextprotocol/server-postgres",
				"${DATABASE_URL}"
			],
			"type": "stdio"
		},
		"github-mpc": {
			"url": "https://api.githubcopilot.com/mcp/",
			"type": "http"
		},
		"notion": {
			"url": "https://mcp.notion.com/mcp",
			"type": "http"
		}
	},
	"inputs": []
}
